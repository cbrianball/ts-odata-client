{
  "name": "ts-odata-client",
  "version": "2.0.0-dev.local",
  "description": "OData TypeScript Client",
  "main": "/index.js",
  "types": "/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "vitest run",
