  "bugs": {
    "url": "https://github.com/cbrianball/ts-odata-client/issues"
  },
  "homepage": "https://github.com/cbrianball/ts-odata-client#readme",
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "chai": "^4.3.4",
    "cpx": "^1.5.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
